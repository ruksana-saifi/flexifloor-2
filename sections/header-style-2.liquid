      {% if section.settings.book-classes %}
      <div class="book-classes "><a href="" class="btn btn-outline-light"> Book Classes</a></div>
        {% endif %}
      {% if section.settings.buy-classes %}
      <div class="buy-classes"><a href="" class=" btn btn-light">Buy Classes</a></div>
        {% endif %}
    </div>
  </div>
</header>

{% schema %}
{
  "name": "Header Style 2",
  "settings": [
    {
      "type": "image_picker",
      "id": "logo1",
      "label": "Select Logo"
    },
      {
      "type": "image_picker",
      "id": "logo2",
      "label": "Logo After Scrolling"
    },
    {
      "type": "checkbox",
      "id": "login-btn",
      "label": "Show Login Button",
      "default": true
    },
    {
      "type": "checkbox",
      "id": "book-classes",
      "label": "Show Book Now Button",
      "default": true
    },
    {
      "type": "checkbox",
      "id": "buy-classes",
      "label": "Show Buy Now Button",
      "default": true
    }
  ],
  "blocks": [
    {
      "name": "Menu Links",
      "type": "select",
      "settings": [
        {
          "type": "text",
          "id": "menu-title",
          "label": "Menu Title"
        },
        {
          "type": "link_list",
          "id": "menu-list",
          "label": "Select Menu"
        }
      ]
    }
  ],
  "presets": [
    {
      "name": "Header Style 2"
    }
  ]
}
{% endschema %}

{% javascript %}
  $('.icon-hamburger').click(function(){
         $('.mega-menu').addClass('active')
     })
     $('.icon-close').click(function(){
         $('.mega-menu').removeClass('active')
     });
{% endjavascript %}
